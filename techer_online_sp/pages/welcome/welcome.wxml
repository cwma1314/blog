<!--index.wxml-->
<view class="container">
  <view class="userinfo">
    <block>
      <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
  <button type="primary" class="load-wel-btn" open-type="getPhoneNumber" lang="zh_CN" bindgetphonenumber="getUserInfo">
    获取用户信息
  </button>
</view>